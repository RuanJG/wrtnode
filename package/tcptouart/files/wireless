config wifi-device  ra0
	option type     ralink
	option mode 	9
	option channel  auto
	option txpower 60
	option ht 	20
	option country US
	
# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 0	
	
config wifi-iface
	option device   ra0
	option network	lan
	option mode     ap
	option ssid     WRTnode4g
	option encryption psk2
	option key 12345678
	option ApCliEnable '0'
	option ApCliSsid 'te-copter'
	option ApCliAuthMode 'WPA2PSK'
	option ApCliEncrypType 'TKIPAES'
	option ApCliPassWord '88888888'
	
